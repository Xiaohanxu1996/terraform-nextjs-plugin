{
  "name": "@ematipico/terraform-nextjs-plugin",
  "version": "0.5.0",
  "description": "A nextjs plugin to generate cloud providers configuration for Terraform",
  "main": "src/index.js",
  "bin": "bin/terranext.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest --maxWorkers=4 --reporters=default --reporters=jest-junit",
    "release": "standard-version",
    "lint": "eslint --ext js ./src ./bin ./tests",
    "lint:fix": "eslint --ext js ./src ./bin ./tests --fix",
    "integration:build": "cd ./integration/app; next build; node ./build.js"
  },
  "typings": "./src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ematipico/terraform-nextjs-plugin.git"
  },
  "keywords": [
    "nextjs",
    "terraform",
    "serverless",
    "plugin",
    "aws",
    "cloud",
    "providers"
  ],
  "author": "Emanuele Stoppa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ematipico/terraform-nextjs-plugin/issues"
  },
  "homepage": "https://github.com/ematipico/terraform-nextjs-plugin#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-unicorn": "^9.1.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard-version": "^6.0.1",
    "tsd": "^0.7.3"
  },
  "dependencies": {
    "cosmiconfig": "^5.2.1",
    "meow": "^5.0.0",
    "prettier": "^1.17.1"
  },
  "engines": {
    "node": ">= 8.5.x"
  },
  "files": [
    "bin/*",
    "src/*"
  ]
}
